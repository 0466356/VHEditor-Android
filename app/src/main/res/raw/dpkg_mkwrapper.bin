#!/data/data/vn.vhn.vsc/files/usr/bin/bash

ROOT=/data/data/vn.vhn.vsc/files

if /data/data/vn.vhn.vsc/files/node -e 'process.exit(require("fs").statSync(process.argv[1]).size>5000 ? 0 : 1)' "$ROOT/usr/bin/dpkg"; then
    mv -f "$ROOT/usr/bin/dpkg" "$ROOT/usr/bin/dpkg.bin-orig"
    echo "Updated dpkg wrapper" >&2
fi

cp -f "$ROOT/usr/bin/dpkg.wrapper" "$ROOT/usr/bin/dpkg"
